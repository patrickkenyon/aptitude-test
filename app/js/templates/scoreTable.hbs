{{#each data}}
<tr class="table-entry">
    <td class="name">{{this.name}}</td>
    <td class="email">{{this.email}}</td>
    <td class="date">{{this.date}}</td>
    <td class="score">{{this.score}}</td>
    <td class="percentage">{{this.percentage}}%</td>
    <td class="time">{{this.time}}</td>
</tr>
{{/each}}